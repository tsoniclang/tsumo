{
  "$schema": "https://tsonic.dev/schema/v1.json",
  "rootNamespace": "Tsumo.Cli",
  "entryPoint": "src/App.ts",
  "sourceRoot": "src",
  "outputDirectory": "generated",
  "outputName": "tsumo",
  "output": {
    "nativeAot": false,
    "trimmed": false,
    "singleFile": false
  },
  "buildOptions": {
    "stripSymbols": true,
    "invariantGlobalization": true
  },
  "dotnet": {
    "typeRoots": ["../../node_modules/@tsonic/globals"],
    "libraries": ["../engine/dist/net10.0/Tsumo.Engine.dll"]
  }
}
