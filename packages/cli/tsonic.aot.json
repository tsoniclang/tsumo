{
  "$schema": "https://tsonic.org/schema/v1.json",
  "rootNamespace": "Tsumo.Cli",
  "entryPoint": "src/cli-main.ts",
  "sourceRoot": "src",
  "outputDirectory": "generated",
  "outputName": "tsumo-aot",
  "output": {
    "nativeAot": true,
    "trimmed": true,
    "singleFile": true
  },
  "buildOptions": {
    "stripSymbols": true,
    "invariantGlobalization": true
  },
  "references": {
    "libraries": ["../engine/dist/net10.0/Tsumo.Engine.dll"]
  }
}
